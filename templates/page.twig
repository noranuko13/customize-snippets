<!doctype html>
<html lang="ja">
<head prefix="og: https://ogp.me/ns# fb: https://ogp.me/ns/fb# article: https://ogp.me/ns/article#">
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ key }} - {{ sitename }}</title>
  <meta property="og:title" content="{{ key }}" />
  <meta property="og:type" content="artcle" />
  <meta property="og:url" content="{{ basepath }}{{ key }}/" />
  <meta property="og:image" content="{{ basepath }}{{ key }}/image.png" />
  <meta property="og:site_name" content="{{ sitename }}" />
  <meta property="og:description" content="{{ description }}" />
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:site" content="@noranyanko13">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/water.css@2/out/water.css">
</head>
<body>
{%- block content -%}{%- endblock -%}
<hr>
<ul>
  {%- for page in pages -%}
    <li><small><a href="{{ basepath }}{{ page.key }}/">{{ page.key }}</a>: {{ page.description }}</small></li>
  {%- endfor -%}
</ul>
</body>
</html>
